{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
  },
  "extends": [
    "airbnb",
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
    "import",
  ],
  "rules": {
    "semi": ["error", "never"],
    "react/jsx-filename-extension": "off",
    "react/require-extension": "off",
    "import/no-extraneous-dependencies": "off",
  }
}
